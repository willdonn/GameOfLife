package Board;

public class Position {

	public int x;
	public int y;
	
	public Position(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public boolean equals(Position position) {
		return this.x == position.x && this.y == position.y;
	}
	
	public int hashCode() {
        int hash = 1;
        final int prime = 13;
        hash = prime * hash +  x;
        hash = prime * hash + y;
        return hash;
    }
	
}
